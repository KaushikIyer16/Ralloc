<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Date</servlet-name>
        <servlet-class>com.ralloc.web.Date</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FileUploadServlet</servlet-name>
        <servlet-class>com.ralloc.web.FileUploadServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SubCode</servlet-name>
        <servlet-class>com.ralloc.web.SubCode</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RoomUpload</servlet-name>
        <servlet-class>com.ralloc.web.RoomUpload</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Date</servlet-name>
        <url-pattern>/DateTime.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FileUploadServlet</servlet-name>
        <url-pattern>/uploadFile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SubCode</servlet-name>
        <url-pattern>/SubCode</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RoomUpload</servlet-name>
        <url-pattern>/GetRoom.do</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>home.html</welcome-file>
    </welcome-file-list>
</web-app>
